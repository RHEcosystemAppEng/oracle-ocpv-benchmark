# tnsnames.ora generated by Ansible
{% for entry in oracle_tns_entries %}
{{ entry.tns_name }} =
  (DESCRIPTION =
    (ADDRESS_LIST =
      (ADDRESS = (PROTOCOL = TCP)(HOST = {{ entry.host }})(PORT = {{ entry.port }}))
    )
    (CONNECT_DATA =
      (SID = {{ entry.sid }})
      (SERVICE_NAME = {{ entry.sid }})
    )
  )

{% endfor %}